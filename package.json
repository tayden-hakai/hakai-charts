{
  "name": "hakai-charts",
  "version": "1.0.0",
  "description": "Reusable charts that display arbitrary data",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require ignore-styles src/**/*.spec.js",
    "preversion": "webpack --config webpack.prod.config.js --progress -p",
    "build": "webpack --config webpack.prod.config.js --progress -p",
    "build:dev": "webpack --config webpack.dev.config.js --progress -p",
    "doc": "./node_modules/.bin/jsdoc2md src/js/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git@hakai.github.com:tayden-hakai/hakai-charts.git"
  },
  "author": "Taylor Denouden",
  "license": "Unlicense",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "ignore-styles": "^5.0.1",
    "jsdoc-to-markdown": "^1.3.7",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "d3": "^4.2.3",
    "d3-array": "^1.0.1",
    "d3-axis": "^1.0.3",
    "d3-brush": "^1.0.2",
    "d3-scale": "^1.0.3",
    "d3-selection": "^1.0.2",
    "d3-shape": "^1.0.3",
    "simple-statistics": "^2.1.0"
  }
}
